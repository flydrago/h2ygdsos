	                	<div class="quanq1 quanqt"></div>
	                    <div class="quanq1"></div>
	                    <div class="quanq1"></div>
	                	 --%>
	                </div>
	            </div>     <!-- jiujiu_nav_nav-->
	            <!--end 9.9-->
	        </div> <!--jiujiu-->
    		
        </div>
        
        <!-- 备注 -->
        <div class="subject_memo">
        	${advertSubject.subjectContent}
        </div>
        
        <div class="resultMessage">
	  		
		</div>
    </div>
 	<script src="<%=uiPath%>jquery/jquery-1.11.2.min.js"></script>
 	<script src="<%=uiPath%>module/advert/js/detail.js"></script>
	<script type="text/javascript">
	
		//区域编码
		var zoneCode = $("#zoneCode").val();//区域编码
		var subjectId = $("#subjectId").val();//主题id
		var memberAccount = $("#memberAccount").val();//主题id
		var opFlag = 0;//0 html操作 1app操作
		var fpPath = "<%=fpPath%>";
		$(function (){
			//加载优惠券数据
			getCouponsData();
			//加载商品数据
			getGoodsData();
		});
		
	</script>   
</body>
</html>
