<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ include file="../../include/taglibs.jsp"%>
<!doctype html>
<html>
<head>
<base href="<%=basePath%>" />
<meta charset="utf-8">
<meta name="viewport" content="width=device-width,initial-scale=1.0" />
<link href="<%=uiPath%>module/advert/css/detail.css" rel="stylesheet">
<title>${advertSubject.subjectName}</title>

</head>
<body>
	<div class="container">
	
		<input name="zoneCode" type="hidden" id="zoneCode" value="${sysUnits.zoneCode}" />
		<input name="memberId" type="hidden" id="memberId" value="${memberUser.id}" />
		<input name="memberAccount" type="hidden" id="memberAccount" value="${memberUser.account}" />
		<input name="subjectId" type="hidden" id="subjectId" value="${advertSubject.id}" />
		<input name="os" type="hidden" id="os" value="${os}" />
        
        <c:if test="${headImgMap == null}">
        	<img class="banner" src="<%=fpPath%>?bn=advertSubjectService&os=wechat&id=${advertSubject.id}&f=<fmt:formatDate value="${advertSubject.updateDate}"  pattern="yyyyMMddHHmmss"/>">
        </c:if>
        
        <c:if test="${headImgMap != null}">
        	<img class="banner" src="<%=fpPath%>?bn=advertSubjectInfoRtService&os=wechat&id=${headImgMap.data_1}&f=<fmt:formatDate value="${headImgMap.create_date}"  pattern="yyyyMMddHHmmss"/>">
        </c:if>
        
        <div id="nav_title" class="nav" style="display: none;">
      		<span>百万礼券大放送</span>
              <img class="left" src="<%=uiPath%>module/advert/images/lijiazuo.png"/>
              <img  class="right" src="<%=uiPath%>module/advert/images/liyou.png"/>
        </div>
        
        <div class="main">
      
            <div class="canvas" id="coupons_div">
  
            </div>   <!--canvas-->
        </div>      <!--main-->
        
  <!--9块9专区-->
        <%-- 促销活动专区开始 --%>
        <div id="activity_zhuanqu">
        
        	<div id="de_block" style="display: none;" class="chunye">
