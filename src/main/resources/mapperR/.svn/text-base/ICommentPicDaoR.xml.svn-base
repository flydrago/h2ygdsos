		<result property="dataVersion" column="data_version" javaType="int" jdbcType="INTEGER" />
		<result property="createDate" column="create_date" javaType="date" jdbcType="TIMESTAMP" />
		<result property="ord" column="ord" javaType="int" jdbcType="INTEGER" />
	</resultMap>
	

	<select id="get" parameterType="long"
		resultMap="commentpicDaoMap">
		select * from tb_comment_pic where id = #{id}
	</select>

	<select id="getList" parameterType="com.h2y.os.entity.CommentPic"
		resultMap="commentpicDaoMap">
		select * from
		tb_comment_pic

		<trim prefix="WHERE" prefixOverrides="AND |OR ">
			
			<if test="id!= null">
			and id = #{id}
			</if>
			<if test="fileName!= null and fileName!=''">
			and file_name = #{fileName}
			</if>
			<if test="diskFileName!= null and diskFileName!=''">
			and disk_file_name = #{diskFileName}
			</if>
			<if test="rootPath!= null and rootPath!=''">
			and root_path = #{rootPath}
			</if>
			<if test="relativePath!= null and relativePath!=''">
			and relative_path = #{relativePath}
			</if>
			<if test="fileSize!= null">
			and file_size = #{fileSize}
			</if>
			<if test="fileSuffix!= null and fileSuffix!=''">
			and file_suffix = #{fileSuffix}
			</if>
			<if test="ifDelete!= null">
			and if_delete = #{ifDelete}
			</if>
			<if test="fileType!= null">
			and file_type = #{fileType}
			</if>
			<if test="dataId!= null and dataId!=''">
			and data_id = #{dataId}
			</if>
			<if test="dataType!= null">
			and data_type = #{dataType}
			</if>
			<if test="dataVersion!= null">
			and data_version = #{dataVersion}
			</if>
			<if test="createDate!= null and createDate!=''">
			and create_date = #{createDate}
			</if>
			<if test="ord!= null">
			and ord = #{ord}
			</if>
		</trim>

	</select>




